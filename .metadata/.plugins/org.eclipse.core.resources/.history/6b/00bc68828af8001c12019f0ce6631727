package com.revature;

public class RevatureEmployee {
	
	String name;
	int age;
	String department;
	
	RevatureEmployee(){
		
	} // RevatureEmployee()
	
	public static void main(String[] args) {
		
		RevatureEmployee ben = new RevatureEmployee();
		
		ben.name = "Ben";
		ben.age = 22;
		ben.department = "Training";
		
	} // main

} // RevatureEmployee
